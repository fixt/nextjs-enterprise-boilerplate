{
  "extends": ["fixt", "plugin:import/errors", "plugin:import/warnings"],
  "plugins": ["jsx-a11y", "json", "markdown"],
  "settings": {
    "react": {
      "createClass": "createReactClass",
      "pragma": "React",
      "version": "16.8.6"
    }
  },
  "rules": {
    "react/no-unescaped-entities": 0
  },
  "overrides": [{
    "files": ["*.md"],
    "rules": {
      "no-console": 0,
      "react/react-in-jsx-scope": 0
    }
  }]
}